{
  "name": "synchronize-tab-scrolling",
  "displayName": "Synchronize Tab Scrolling",
  "version": "2.7.2",
  "private": true,
  "description": "Sync scroll positions across browser tabs - Perfect for comparing documents, code changes, or data side by side.",
  "author": "jaem1n207 <tech.jmtt@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaem1n207/synchronize-tab-scrolling.git"
  },
  "bugs": {
    "url": "https://github.com/jaem1n207/synchronize-tab-scrolling/issues"
  },
  "homepage": "https://github.com/jaem1n207/synchronize-tab-scrolling#readme",
  "keywords": [
    "sync",
    "scroll",
    "scroll-sync",
    "tab-sync",
    "productivity-tool",
    "synchronized-tabs",
    "scroll-synchronization"
  ],
  "type": "module",
  "scripts": {
    "--------------------------------dev-----------------------------": "",
    "dev": "pnpm run clear && cross-env NODE_ENV=development run-p dev:*",
    "dev-firefox": "pnpm run clear && cross-env NODE_ENV=development EXTENSION=firefox run-p dev:*",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:background": "pnpm run build:background -- --mode development",
    "dev:web": "vite",
    "dev:js": "pnpm run build:js -- --mode development",
    "--------------------------------prod-----------------------------": "",
    "build": "cross-env NODE_ENV=production run-s clear i18n:validate build:web build:prepare build:background build:js",
    "build:prepare": "esno scripts/prepare.ts",
    "build:background": "vite build --config vite.config.background.mts",
    "build:web": "vite build",
    "build:js": "vite build --config vite.config.content.mts",
    "--------------------------------release-----------------------------": "",
    "pack": "cross-env NODE_ENV=production run-p pack:*",
    "pack:zip": "rimraf extension.zip && jszip-cli add extension/* -o ./extension.zip",
    "pack:crx": "crx pack extension -o ./extension.crx",
    "pack:xpi": "cross-env WEB_EXT_ARTIFACTS_DIR=./ web-ext build --source-dir ./extension --filename extension.xpi --overwrite-dest",
    "--------------------------------ship-----------------------------": "",
    "start:chromium": "web-ext run --source-dir ./extension --target=chromium",
    "start:firefox": "web-ext run --source-dir ./extension --target=firefox-desktop",
    "--------------------------------test-----------------------------": "",
    "test": "vitest test",
    "test:e2e": "playwright test",
    "--------------------------------tools-----------------------------": "",
    "clear": "rimraf --glob extension/dist extension/manifest.json extension.*",
    "clean": "find . -type f -name 'vite.config.*.timestamp-*' -ls -delete 2>/dev/null",
    "health": "lefthook run health",
    "lint:fix": "NODE_OPTIONS='--experimental-strip-types' eslint . --fix --flag unstable_native_nodejs_ts_config",
    "format:fix": "pnpm prettier --write .",
    "typecheck": "pnpm tsc --noEmit",
    "i18n:validate": "esno scripts/validate-i18n.ts",
    "cz": "czg"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@sentry/browser": "^10.29.0",
    "@sentry/react": "^9.32.0",
    "@sentry/vite-plugin": "^3.5.0",
    "@tanstack/react-query": "^5.90.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "es-hangul": "^2.3.8",
    "motion": "^12.23.24",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@ffflorian/jszip-cli": "^3.8.2",
    "@iconify/json": "^2.2.353",
    "@playwright/test": "^1.53.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.33",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/webextension-polyfill": "^0.12.1",
    "@unocss/preset-wind4": "^66.3.2",
    "@unocss/reset": "^66.3.2",
    "@vitejs/plugin-react": "^4.6.0",
    "chokidar": "^4.0.1",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "czg": "^1.12.0",
    "esbuild": "^0.27.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.4.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "esno": "^4.8.0",
    "fs-extra": "^11.2.0",
    "globals": "^16.2.0",
    "jsdom": "^25.0.1",
    "kolorist": "^1.8.0",
    "lefthook": "^1.11.14",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "rimraf": "^6.0.1",
    "semantic-release": "^25.0.2",
    "shadcn": "^3.5.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.49.0",
    "unocss": "^66.3.2",
    "unocss-preset-animations": "^1.1.0",
    "unocss-preset-shadcn": "^1.0.1",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-icons": "^22.1.0",
    "vite": "^6.0.3",
    "vitest": "^2.1.8",
    "web-ext": "^8.8.0",
    "webext-bridge": "^6.0.1",
    "webextension-polyfill": "^0.12.0"
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a"
}
